{
  "name": "vxe-table-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "http80":"http-server ./docs -p 80 -a 0.0.0.0",
    "build:v1": "cd v1 && npm run build:main",
    "build:v2": "cd v2 && npm run build:main",
    "build:v3": "cd v3 && npm run build:main",
    "build:v4": "cd v4 && npm run build:main",
    "build:other3": "cd other3 && npm run build:main",
    "build:other4": "cd other4 && npm run build:main",
    "update:v1": "cd v1 && npm install --legacy-peer-deps",
    "update:v2": "cd v2 && npm install --legacy-peer-deps",
    "update:v3": "cd v3 && npm install --legacy-peer-deps",
    "update:v4": "cd v4 && npm install --legacy-peer-deps",
    "update:other3": "cd other3 && npm install --legacy-peer-deps",
    "update:other4": "cd other4 && npm install --legacy-peer-deps",
    "update:all": "npm run update:other3 && npm run update:other4 && npm run update:v1 && npm run update:v2 && npm run update:v3 && npm run update:v4",
    "build:all": "npm run build:other3 && npm run build:other4 && npm run build:v1 && npm run build:v2 && npm run build:v3 && npm run build:v4",
    "build:docs": "gulp build_docs && npm run build:zip",
    "build:plugins": "gulp build_docs && gulp build_plugins_docs && npm run build:zip",
    "build:zip": "gulp build_zip"
  },
  "devDependencies": {
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-concat": "2.6.1",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-zip": "^5.1.0",
    "uglify-js": "^3.17.4"
  }
}
